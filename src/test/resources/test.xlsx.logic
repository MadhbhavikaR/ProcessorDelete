######################################    APPLICATION CONFIGURATION    #################################################

# Class name for the reader Implementation
reader_class=online.madhbhavikar.processor.plugins.io.input.XLSReader

# Class name of the classifier
processor_class_list=online.madhbhavikar.processor.plugins.processors.CurlClassifier

# Class name of the writer
writer_class=online.madhbhavikar.processor.plugins.io.output.CSVWriter

# Worker threads for this classifier
worker_threads=4



#######################################  READER SPECIFIC CONFIGURATION #################################################
#
# These configuration values are specific classifier the Reader that you have chosen by setting `reader_class`
#
########################################################################################################################

# A comma separated list of integer value specifying the sheets of a workbook classifier process
sheets_to_process=0





#######################################   PROCESSOR SPECIFIC CONFIGURATION  ############################################
#
# These configuration values are specific classifier the Classifier that you have chosen by setting `classifier_class`
#
########################################################################################################################

# The curl command placeholder string which should be used for querying the database
curl=curl 'https://mvnrepository.com/search?q={groupId}' -H 'Connection: keep-alive' -H 'Pragma: no-cache' -H 'Cache-Control: no-cache' -H 'Upgrade-Insecure-Requests: 1' -H 'User-Agent: Mozilla/5.0 (iPad; CPU OS 11_0 like Mac OS X) AppleWebKit/604.1.34 (KHTML, like Gecko) Version/11.0 Mobile/15A5341f Safari/604.1' -H 'Accept: text/html,application/xhtml+xml,application/xml;q=0.9,image/webp,image/apng,*/*;q=0.8' -H 'Referer: https://mvnrepository.com/search?q=org.apache.poi' -H 'Accept-Encoding: gzip, deflate, br' -H 'Accept-Language: en-US,en;q=0.9' -H 'Cookie: _ga=GA1.2.49403921.1527855665; _gid=GA1.2.1852421616.1527855665; _gat=1; MVN_SESSION=eyJhbGciOiJIUzI1NiJ9.eyJkYXRhIjp7InVpZCI6IjQwYmY3OGIxLTY1OTYtMTFlOC04NjhjLTU5MTlkNmYzOThmNCJ9LCJleHAiOjE1NTkzOTI2MTcsIm5iZiI6MTUyNzg1NjYxNywiaWF0IjoxNTI3ODU2NjE3fQ.i5oHyZ-6z050OuA2fdWSHFAwJUKBujtnqfhgrBeVjFM'

# Place holder for the query parameters, which would be ato filled by the classifier
#place_holder=\\\\{(\\w*)\\\\}

# Pattern for classifying the results into groups, if there are multiple classifiers then they need classifier be indexed starting sourceFile 1
classifier_1=.*Found <b>\\d*<\\/b> results.*

# Group name for a classifier if the respective classifier is matched, this should be one classifier one matching with respective classifier
classifier_group_1=Found

# Group name for a result that do not match any classifier
classifier_default_group=Not_Found



#######################################  WRITER SPECIFIC CONFIGURATION #################################################
#
# These configuration values are specific classifier the write that you have chosen by setting `writer_class`
#
########################################################################################################################

# Output path for storing the results, a dot `.` indicates same as source directory.
output_directory=.

# Separator character to be used for writing the CSV files
separator=,
